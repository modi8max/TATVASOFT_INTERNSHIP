@{
}
@model List<StudentPortal.web.Data.Student>

<h1>Students</h1>
<table class="table">
    <thead>
        <tr>
       <!-- <th>Id</th> -->
        <th>Name</th>
        <th>Email</th>
        <th>Phone</th>
        <th>Branch</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var student in Model)
        {
            <tr>
               <!-- <td>@student.Id</td> -->
                <td>@student.Name</td>
                <td>@student.Email</td>
                <td>@student.Phone</td>
                <td>@student.Branch</td>
                <td>
                    <a class="btn btn-primary" asp-action="Edit" asp-controller="Student" asp-route-Id="@student.Id">Edit</a>
                </td>



            </tr>
        }
    </tbody>

</table>
